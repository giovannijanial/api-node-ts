generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Student {
  id          String      @id     @default(uuid())
  name        String
  clas        Int
  gender      String
  created_at  DateTime    @default(now())     

  @@map("students")
}